{
  "name": "cc-demo",
  "version": "2.1.0",
  "description": "",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "commitplease": "^3.2.0",
    "husky": "^1.3.1",
    "standard-version": "^5.0.1"
  },
  "scripts": {
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superliuwr/cc-demo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/superliuwr/cc-demo/issues"
  },
  "homepage": "https://github.com/superliuwr/cc-demo#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitplease .git/COMMIT_EDITMSG"
    }
  },
  "commitplease": {
    "limits": {
      "firstLine": "72",
      "otherLine": "80"
    },
    "nohook": true,
    "markerPattern": "^(feat|fix|polish|docs|style|refactor|perf|test|workflow|ci|chore|types|build)",
    "ticketPattern": "((.*#|gh-|[A-Z]{2,}-)[0-9]+ |(\\d\\.\\d\\.\\d))"
  }
}
